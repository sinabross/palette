<!--제품요청목록-->
<% if @askfor %>
  <%= will_paginate @askfor,
                    :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                    inner_window: 2, outer_window: 0,
                    :previous_label => '<', :next_label => '>'%>
<% end %>

<!--회원목록보기(관리자기능)-->
<% if @user %>
  <%= will_paginate @user,
                    :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                    inner_window: 2, outer_window: 0,
                    :previous_label => '<', :next_label => '>'%>
<% end %>

<!--찜 목록보기(마이페이지)-->
<% if @like_list %>
  <%= will_paginate @like_list,
                    :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                    inner_window: 2, outer_window: 0,
                    :previous_label => '<', :next_label => '>'%>
<% end %>

<!--제품 사용 후기-->
<% if @review %>
  <%= will_paginate @review,
                    :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                    inner_window: 2, outer_window: 0,
                    :previous_label => '<', :next_label => '>'%>
<% end %>

<!--립스틱 제품 리스트-->
<% if @list %>
  <%= will_paginate @list,
                    :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                    inner_window: 2, outer_window: 0,
                    :previous_label => '<', :next_label => '>'%>
<% end %>

<!--검색 결과 리스트 -->
<% unless @result.blank? && @result2.blank?%>
  <%= will_paginate @result,
                    :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                    inner_window: 2, outer_window: 0,
                    :previous_label => '<', :next_label => '>'%>
<%else%>
  <% if @result %>
        <%= will_paginate @result,
                          :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                          inner_window: 2, outer_window: 0,
                          :previous_label => '<', :next_label => '>'%>
  <% end %>
  
  <% if @result2 %>
        <%= will_paginate @result2,
                          :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                          inner_window: 2, outer_window: 0,
                          :previous_label => '<', :next_label => '>'%>
  <% end %>
<%end%>

<!--새소식-->
<% if @notices %>
  <%= will_paginate @notices,
                    :class => "flickr_pagination col-lg-12 col-md-12 col-xs-12 col-sm-12",
                    inner_window: 2, outer_window: 0,
                    :previous_label => '<', :next_label => '>'%>
<% end %>
